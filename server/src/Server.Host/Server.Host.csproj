<Project Sdk="Microsoft.NET.Sdk.Web">
    <PropertyGroup>
        <PackageId>Server.Host</PackageId>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Exta.Api</Description>
        <TargetFramework>net7.0</TargetFramework>
        <OutputType>Exe</OutputType>
        <DebugType>portable</DebugType>
        <LangVersion>11.0</LangVersion>
        <WarningsAsErrors>true</WarningsAsErrors>
        <IsPackable>false</IsPackable>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="../Server.Application/Server.Application.csproj" />
        <ProjectReference Include="../Server.ViewModels/Server.ViewModels.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Annium.Architecture.Http" Version="0.1.0" />
        <PackageReference Include="Annium.AspNetCore.Extensions" Version="0.1.0" />
        <PackageReference Include="Annium.Configuration.Abstractions" Version="0.1.0" />
        <PackageReference Include="Annium.Configuration.CommandLine" Version="0.1.0" />
        <PackageReference Include="Annium.Configuration.Yaml" Version="0.1.0" />
        <PackageReference Include="Annium.Core.DependencyInjection" Version="0.1.0" />
        <PackageReference Include="Annium.Infrastructure.Hosting" Version="0.1.0" />
        <PackageReference Include="Annium.Logging.Console" Version="0.1.0" />
        <PackageReference Include="Annium.Logging.Microsoft" Version="0.1.0" />
        <PackageReference Include="Annium.Net.Http" Version="0.1.0" />
        <PackageReference Include="Annium.NodaTime.Serialization.Json" Version="0.1.0" />
        <PackageReference Include="XRest.Plugins.AspNetCore" Version="0.1.0" />
    </ItemGroup>
</Project>