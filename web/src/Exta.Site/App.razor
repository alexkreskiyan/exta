@using Annium.Blazor.Css
@using Exta.Site.Public.Layouts
@using Exta.Site.Public.Layouts.MinimalCentric
@using Exta.Site.Public.Pages.NotFound;
@using Microsoft.AspNetCore.Components.Routing
<Router AppAssembly="@typeof(Program).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(MinimalCentricLayout)">
            <NotFoundPage />
        </LayoutView>
    </NotFound>
</Router>
<StyleSheet />